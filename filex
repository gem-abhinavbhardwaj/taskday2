pipeline {
  agent any
  stages {
    stage('Build') {
      steps {
        sh 'echo "Building..."'
        
       dir("${env.WORKSPACE}") {
          sh 'ls'          // Use sh to run the rm command to delete the @tmp and _ws_cleanup folders
          sh 'rm -rf *@tmp *_ws_cleanup'
        }
       

      }
    }
    stage('Test') {
      steps {
        sh 'echo "Testing..."'
      }
    }
    stage('Deploy') {
      steps {
        sh 'echo "Deploying..."'
      }
    }
  }
}
