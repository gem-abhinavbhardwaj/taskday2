pipeline {
  agent any
  stages {
    stage('Build') {
      steps {
        sh 'echo "Building..."'
      }
      post {
        always {
          cleanWs()
        }
      }
    }
    stage('Test') {
      steps {
        sh 'echo "Testing..."'
      }
      post {
        always {
          cleanWs()
        }
      }
    }
    stage('Deploy') {
      steps {
        sh 'echo "Deploying..."'
      }
      post {
        always {
          cleanWs()
        }
      }
    }
  }
}
