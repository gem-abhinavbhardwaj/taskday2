pipeline {
  agent any
  stages {

 

    stage('Build') {
      steps {
        sh 'echo "Building..."'
 post {
      always {
        dir("${env.WORKSPACE}@tmp") {
          deleteDir()
        }
      }
    }
      }
    }
    stage('Test') {
      steps {
        sh 'echo "Testing..."'
      }
    }
    stage('Deploy') {
      steps {
        sh 'echo "Deploying..."'
      }
    }

 

  }
 

 

}
